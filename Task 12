#include <iostream>
#include <cmath>
#include <vector>
#include <string>
using namespace std;

int main(){
    long long k, i, j, g;
    vector <int> v=0;
    for (i=1;v.size()<500;i++){
        v.clear();
        k=0;
        for (j=1;j<i;j++){
            k+=j;
            }
            for (int n=1;n<=sqrt(k);n++){
                if (k%n==0){
                    v.push_back(n);
                    v.push_back(k/n);
                     }
                }       
        }
        
        cout<<k<<"  /  "<<v.size()<<"  /  ";
        cout<<endl;
        
    for (g=0;g<v.size();g++){
           cout<<v[g]<<" ";
        }    
}
